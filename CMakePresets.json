{
  "version": 4,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21
  },
  "configurePresets": [
    {
      "name": "msvc-qt-release",
      "displayName": "MSVC + Qt 6.9.2 (Release)",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_CXX_STANDARD": "17",
        "CMAKE_PREFIX_PATH": "C:/Qt/6.9.2/msvc2022_64"
      }
    },
    {
      "name": "msvc-qt-debug",
      "displayName": "MSVC + Qt 6.9.2 (Debug)",
      "inherits": "msvc-qt-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      },
      "binaryDir": "${sourceDir}/out/build/${presetName}"
    }
  ]
}
